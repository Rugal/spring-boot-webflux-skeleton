# getXXX: to get a single object by its id
# findXXX: to find/search for a list of object by predicate with pagination

input NewStudent {
  name: String!
}

input UpdateStudent {
  name: String!
}

type Student {
  id: Int!
  name: String!
  createAt: Int!
  updateAt: Int!

  delete: Boolean!
  update(input: UpdateStudent!): Student!
}
