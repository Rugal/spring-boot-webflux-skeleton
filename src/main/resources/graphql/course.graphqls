# getXXX: to get a single object by its id
# findXXX: to find/search for a list of object by predicate with pagination

input NewCourse {
  name: String!
}

input UpdateCourse {
  name: String!
}

type Course {
  id: Int!
  name: String!
  createAt: Int!
  updateAt: Int!

  delete: Boolean!
  update(input: UpdateCourse!): Course!
}
