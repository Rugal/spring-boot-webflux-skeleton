{
  "fieldsWithResolvers": [
    "Course.delete",
    "Course.update",
    "Student.delete",
    "Student.update",
    "Registration.course",
    "Registration.delete",
    "Registration.student",
    "Registration.update"
  ],
  "parametrizedResolverAnnotations": [
    "org.springframework.graphql.data.method.annotation.SchemaMapping(typeName=\"{{TYPE_NAME}}\")"
  ],
  "apiInterfaceStrategy": "DO_NOT_GENERATE",
  "customTypesMapping": {
  },
  "apiReturnType": "reactor.core.publisher.Mono",
  "apiReturnListType": "reactor.core.publisher.Flux",
  "modelNameSuffix": "Dto",
  "modelValidationAnnotation": "@jakarta.validation.constraints.NotNull",
  "customAnnotationsMapping": {
    "^Query\\.\\w+$": ["org.springframework.graphql.data.method.annotation.QueryMapping"],
    "^Mutation\\.\\w+$": ["org.springframework.graphql.data.method.annotation.MutationMapping"],
    "^Subscription\\.\\w+$": ["org.springframework.graphql.data.method.annotation.SubscriptionMapping"]
  },
  "resolverArgumentAnnotations": ["org.springframework.graphql.data.method.annotation.Argument"],
  "generateDataFetchingEnvironmentArgumentInApis": true
}
